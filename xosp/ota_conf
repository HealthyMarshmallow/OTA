# Common OTA url for LG G2 device
ota_url=https://raw.githubusercontent.com/HealthyMarshmallow/OTA/mm/xosp/ota.xml
release_type=Stable
device_name=ro.xosp.device

# Example ro.xosp.display.version=XOSP-5.4-UNOFFICIAL-20160424-211748-d802
# version_position is zero based and it's compared to Filename in OTA XML
version_source=ro.xosp.display.version
version_delimiter=-
version_position=3
version_format=yyyyMMdd
