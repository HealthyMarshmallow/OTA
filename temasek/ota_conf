# Common OTA url for LG G2 device
ota_url=https://raw.githubusercontent.com/HealthyMarshmallow/OTA/mm/temasek/ota.xml
release_type=Stable
device_name=ro.product.device

# Example ro.modversion=13.0-20160523-084537-UNOFFICIAL-temasek-d802
# version_position is zero based and it's compared to Filename in OTA XML
version_source=ro.modversion
version_delimiter=-
version_position=1
version_format=yyyyMMdd
